<% #　アカウント検索画面 %>
<%= will_paginate %>

<%= form_tag(users_path, method: :get) do %>
<div class="search-container">
  <%= text_field_tag :search, params[:search], class: 'search-field' %>
  <%= image_submit_tag('search.png', class: 'submit-button') %>
</div>
<% end %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>
